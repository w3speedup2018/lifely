{% if section.settings.heading != blank  or section.settings.sub_heading != blank or section.blocks != blank %}
	<section class="home_image_slider collection_image_slider {{section.settings.section_class}}">
      	<div class="home_image_slider_container">
      	  <div class="row">
          	{% if section.settings.heading != blank or section.settings.sub_heading != blank%}
                <div class="col-12">
                    <div class="img_slider_heading {% if section.settings.mbheading != blank %}hide_on_mobile_review{% endif %}">
                        <div class="mobile_insta_icon">
                            <svg width="44" height="43" viewBox="0 0 44 43" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M31.2598 43H12.7369C5.98824 43 0.5 37.5085 0.5 30.7598V12.2369C0.5 5.49153 5.98824 0 12.7369 0H31.2598C38.0085 0 43.5 5.49153 43.5 12.2369V30.7598C43.5 37.5085 38.0085 43 31.2598 43ZM12.7369 3.68618C8.02008 3.68618 4.1829 7.52336 4.1829 12.2402V30.7631C4.1829 35.4799 8.02008 39.3171 12.7369 39.3171H31.2598C35.9766 39.3171 39.8138 35.4799 39.8138 30.7631V12.2369C39.8138 7.52008 35.9766 3.6829 31.2598 3.6829H12.7369V3.68618Z" fill="#AFBAA1"/>
                            <path d="M21.9995 32.5093C15.927 32.5093 10.9902 27.5692 10.9902 21.5C10.9902 15.4275 15.927 10.4907 21.9995 10.4907C28.0721 10.4907 33.0089 15.4275 33.0089 21.5C33.0089 27.5692 28.0688 32.5093 21.9995 32.5093ZM21.9995 14.1768C17.9621 14.1768 14.6764 17.4626 14.6764 21.5C14.6764 25.5374 17.9621 28.8231 21.9995 28.8231C26.037 28.8231 29.3227 25.5374 29.3227 21.5C29.3227 17.4626 26.037 14.1768 21.9995 14.1768Z" fill="#AFBAA1"/>
                            <path d="M36.1272 9.86043C36.1272 11.3211 34.9423 12.5061 33.4816 12.5061C32.0209 12.5061 30.8359 11.3211 30.8359 9.86043C30.8359 8.39975 32.0209 7.21478 33.4816 7.21478C34.9423 7.21478 36.1272 8.39975 36.1272 9.86043Z" fill="#AFBAA1"/>
                            </svg>
                        </div>  
                        <div class="be-heading text-center">
                            {% if section.settings.heading != blank %}
                                <h2>{{ section.settings.heading }}</h2>
                            {% endif %} 
                            	{% if section.settings.sub_heading != blank %}
                      		<p>{{ section.settings.sub_heading }} </p>
                      	{% endif %}
                        </div>     
               	   </div>
                   {% if section.settings.mbheading != blank %}
                  		<div class="review_mobile_sec" style="display:none;">
                  			<div class="mobile_insta_icon">
                                <svg width="44" height="43" viewBox="0 0 44 43" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path d="M31.2598 43H12.7369C5.98824 43 0.5 37.5085 0.5 30.7598V12.2369C0.5 5.49153 5.98824 0 12.7369 0H31.2598C38.0085 0 43.5 5.49153 43.5 12.2369V30.7598C43.5 37.5085 38.0085 43 31.2598 43ZM12.7369 3.68618C8.02008 3.68618 4.1829 7.52336 4.1829 12.2402V30.7631C4.1829 35.4799 8.02008 39.3171 12.7369 39.3171H31.2598C35.9766 39.3171 39.8138 35.4799 39.8138 30.7631V12.2369C39.8138 7.52008 35.9766 3.6829 31.2598 3.6829H12.7369V3.68618Z" fill="#AFBAA1"/>
                                <path d="M21.9995 32.5093C15.927 32.5093 10.9902 27.5692 10.9902 21.5C10.9902 15.4275 15.927 10.4907 21.9995 10.4907C28.0721 10.4907 33.0089 15.4275 33.0089 21.5C33.0089 27.5692 28.0688 32.5093 21.9995 32.5093ZM21.9995 14.1768C17.9621 14.1768 14.6764 17.4626 14.6764 21.5C14.6764 25.5374 17.9621 28.8231 21.9995 28.8231C26.037 28.8231 29.3227 25.5374 29.3227 21.5C29.3227 17.4626 26.037 14.1768 21.9995 14.1768Z" fill="#AFBAA1"/>
                                <path d="M36.1272 9.86043C36.1272 11.3211 34.9423 12.5061 33.4816 12.5061C32.0209 12.5061 30.8359 11.3211 30.8359 9.86043C30.8359 8.39975 32.0209 7.21478 33.4816 7.21478C34.9423 7.21478 36.1272 8.39975 36.1272 9.86043Z" fill="#AFBAA1"/>
                                </svg>
                            </div>
                          	<div class="be-heading text-center">
                                {% if section.settings.mbheading != blank %}
                                    <h2>{{ section.settings.mbheading }}</h2>
                                {% endif %} 
                              	 {% if section.settings.mbsub_heading != blank %}
                              			<p>{{ section.settings.mbsub_heading }} </p>
                              	 {% endif %} 
                            </div>
                  		</div>  
                   {% endif %}  
                      
                </div>
          	{% endif %}
            {% if section.blocks and section.blocks != blank %}
                <div class="col-12">
                  <div class="image-slider-contet">
                      <div class="homeImage_slider">	
                        {%- for block in section.blocks -%}
                          {% if block.settings.icon_image != blank %}
								<div class="image_slide">
                                  <a href="{{ block.settings.icon_image_url }}"><img src="{{ block.settings.icon_image | img_url: 'master' }}" alt="{{ block.settings.icon_image.alt }}" class="img-fluid" /></a>
                        		</div>  
                           {% endif %}	
                        {% endfor %}
                      </div>  
                  </div>  
            	</div>  
             {% endif %}
      
          </div>  
      	</div> 
	</section>
{% endif %}

{% schema %}
  {
    "name": "Image Slider",
    "class":"review_image_slider_settings",
    "settings": [
          {
            "id": "heading",
            "type": "text",
            "label": "Heading"
          },
          {
            "id": "sub_heading",
            "type": "textarea",
            "label": "Content"
          },
		  {
            "id": "mbheading",
            "type": "text",
            "label": "Mobile Heading"
          },
          {
            "id": "mbsub_heading",
            "type": "textarea",
            "label": "Mobile Content"
          },
          {
            "type": "text",
            "id": "section_class",
            "label": "section class"
          }
	],
	"blocks": [
        {
          "type": "image-slide",
          "name": "Slide",
          "settings": [
              {
                "type": "image_picker",
                "id": "icon_image",
                "label": "Image"    
              },
              {
                "type": "text",
                "id": "icon_image_url",
                "label": "Image URL"    
              }
          ]
        }
      ]		

  }
{% endschema %}

                                  